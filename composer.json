{
    "name": "lechimp-p/php2js",
    "description": "Compiles sane PHP to javascript.",
    "keywords": ["compiler", "javascript", "client"],
    "type": "library",
    "license": "GPL-3.0-or-later",
    "homepage": "https://github.com/lechimp-p/php2js",
    "authors": [
        {
            "name": "Richard Klees",
            "email": "richard.klees@rwth-aachen.de"
        }
    ],
    "support": {
        "issues": "https://github.com/lechimp-p/php2js/internal/issues",
        "source": "https://github.com/lechimp-p/php2js"
    },
    "autoload" : {
        "psr-4" : {
            "Lechimp\\PHP2JS\\" : "src",
            "Lechimp\\PHP2JS\\Test\\" : "tests",
            "Lechimp\\PHP2JS\\JS_Tests\\" : "js_tests"
        }
    },
    "require": {
        "nikic/php-parser" : "4.*.*",
        "pimple/pimple" : "3.2.*",
        "symfony/console" : "3.4.*",
        "psr/log" : "1.0.0",
        "composer/composer" : "1.10.26"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.1"
    },
    "bin" : [
        "bin/php2js"
    ]
}
